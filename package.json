{
  "name": "skift",
  "description": "Split testing tool for the Web",
  "version": "3.0.0",
  "author": "Trustpilot A/S",
  "bugs": {
    "url": "https://github.com/trustpilot/skift/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/git": "6.0.1",
    "@types/jasmine": "2.5.52",
    "cross-env": "5.1.5",
    "css-loader": "0.28.11",
    "css-to-string-loader": "0.1.3",
    "express": "4.16.3",
    "jasmine-core": "2.6.3",
    "jquery": "3.3.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-jasmine": "1.1.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "querystringify": "1.0.0",
    "semantic-release": "15.6.0",
    "ts-loader": "2.2.2",
    "tslib": "1.9.0",
    "tslint": "5.10.0",
    "typescript": "2.8.3",
    "webpack": "3.0.0"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "homepage": "https://github.com/trustpilot/skift#readme",
  "license": "MIT",
  "main": "dist/skift.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustpilot/skift.git"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "ci:travis": "npm run lint && npm run test",
    "cz": "git-cz",
    "lint": "tslint 'src/**/*.ts' --type-check --project ./",
    "start": "node examples/serve.js",
    "test": "karma start",
    "ts": "tsc"
  },
  "types": "types/index.d.ts",
  "release": {
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "dist/**/*.{js|css|map}",
          "types"
        ],
        "message": "chore: ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ]
  }
}
