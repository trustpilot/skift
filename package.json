{
  "name": "skift",
  "description": "Split testing tool for the Web",
  "version": "2.1.0",
  "author": "Trustpilot A/S",
  "main": "dist/skift.js",
  "types": "types/index.d.ts",
  "homepage": "https://github.com/trustpilot/skift#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustpilot/skift.git"
  },
  "bugs": {
    "url": "https://github.com/trustpilot/skift/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:dev && npm run build:prod",
    "build:dev": "webpack --config ./config/webpack.dev.js",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "clean": "rimraf dist/ yarn-error.log npm-debug.log",
    "cz": "git-cz",
    "lint": "tslint 'src/**/*.ts' --project ./",
    "precz": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "webpack-serve --config ./config/webpack.dev.js",
    "test": "jest --config ./config/jest.config.js"
  },
  "dependencies": {
    "querystringify": "2.0.0"
  },
  "devDependencies": {
    "ajv": "6.5.1",
    "css-loader": "0.28.11",
    "css-to-string-loader": "0.1.3",
    "cz-conventional-changelog": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.2.0",
    "jquery": "3.3.1",
    "rimraf": "2.6.2",
    "semantic-release": "15.6.0",
    "ts-jest": "22.4.6",
    "ts-loader": "4.4.2",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.12.2",
    "webpack-command": "0.3.1",
    "webpack-serve": "1.0.4"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
