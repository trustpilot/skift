    "express": "4.16.3",
    "jest": "23.1.0",
    "semantic-release": "15.6.0",
    "ts-jest": "22.4.6",
    "ts-loader": "4.4.1",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "uglifyjs-webpack-plugin": "1.2.6",
