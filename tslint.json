{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-console": [true, "time", "timeEnd", "trace"],
    "max-line-length": [true, 120],
    "quotemark": [true, "single", "avoid-escape"],
    "interface-name": [true, "never-prefix"],
    "variable-name": [true, "allow-leading-underscore", "ban-keywords", "check-format"]
  }
}
